    <Collection>
      <Record Type="UI.DataField">
        <PropertyValue Property="Value" Path="Zyear"/>
        <PropertyValue Property="Label" String="Évszám"/>
      </Record>
      <Record Type="UI.DataField">
        <PropertyValue Property="Value" Path="Zmonth"/>
        <PropertyValue Property="Label" String="Hónap"/>
      </Record>
      <Record Type="UI.DataField">
