<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="selectDialogToAddress"
		title="Címek"
		items="{/ZbbvhAddresslistSet}"
		search=".onAddressValueHelpSearch"
		confirm=".onToAddressValueHelpClose"
		cancel=".onToAddressValueHelpClose"
		searchPlaceholder="Keresett cím kódja">
		<StandardListItem id="_IDGenStandardListItemAddressTo"
			iconDensityAware="false"
			iconInset="false"
			title="{Country} {PostCode} {City}, {Street} {HouseNumber}. ez a to ablak"
			description="{SerialNumber}"/>
	</SelectDialog>
</core:FragmentDefinition>