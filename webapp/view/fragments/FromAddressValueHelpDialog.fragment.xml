<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="selectDialogFromAddress"
		title="Címek"
		items="{/ZbbvhAddresslistSet}"
		search=".onAddressValueHelpSearch"
		confirm=".onFromAddressValueHelpClose"
		cancel=".onFromAddressValueHelpClose"
		searchPlaceholder="Keresett cím kódja">
		<StandardListItem id="StandardListItemAddressFrom"
			iconDensityAware="false"
			iconInset="false"
			title="{Country} {PostCode} {City}, {Street} {HouseNumber}."
			description="{SerialNumber}"/>
	</SelectDialog>
</core:FragmentDefinition>